@model IEnumerable<DarulAman.Models.tbl_SchoolCourse>

@{
    ViewBag.Title = "Index";
}

<div class="midde_cont">
    <div class="container-fluid">
        <div class="row column_title">
            <div class="col-md-12">
                <div class="page_title">
                    <h2>Index</h2>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="container-fluid">
    <div class="row">

        <div class="col-lg-6 col-md-6 col-sm-6">
            <p class="heading">School Course</p>
        </div>
        <div class="col-lg-6 col-md-6 col-sm-6">

            <a style="float:right;" class="btn btn-outline-info btn-lg" href="@Url.Action("Create", "tbl_SchoolCourse")"><b>Create New</b> </a>
        </div>

    </div>
</div>
<br />

<table class="table">
    <tr>
        <th>
            COURSE_NAME
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.COURSE_NAME)
    </td>
    <td>
        <a href="@Url.Action("Edit", "tbl_SchoolCourse", new { id=item.COURSE_ID})">
            <i class="fa fa-edit btn btn-success  "></i>
        </a>|
        <a href="@Url.Action("Details", "tbl_SchoolCourse", new { id=item.COURSE_ID })">
            <i class="fa fa-book btn btn-warning"></i>
        </a>|
        <a href="@Url.Action("Delete", "tbl_SchoolCourse", new { id=item.COURSE_ID })">
            <i class="fa fa-trash btn btn-danger"></i>
        </a>
    </td>
   

</tr>
    }

</table>
