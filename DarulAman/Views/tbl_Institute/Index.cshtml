@model IEnumerable<DarulAman.Models.tbl_Institute>

@{
    ViewBag.Title = "Index";
}

<div class="midde_cont">
    <div class="container-fluid">
        <div class="row column_title">
            <div class="col-md-12">
                <div class="page_title">
                    <h2>Index</h2>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="container-fluid">
    <div class="row">

        <div class="col-lg-6 col-md-6 col-sm-6">
            <p class="heading">Institute</p>
        </div>
        <div class="col-lg-6 col-md-6 col-sm-6">

            <a style="float:right;" class="btn btn-outline-info btn-lg" href="@Url.Action("Create", "tbl_Institute")"><b>Create New</b> </a>
        </div>

    </div>
</div>
<br />


<table class="table">
    <tr>
        <th style="color:black">
            INSTITUTE_EMAIL
        </th>
        <th style="color:black">
            INSTITUTE_CONTACT
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.INSTITUTE_EMAIL)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.INSTITUTE_CONTACT)
    </td>
   
    <td>


        <a href="@Url.Action("Edit", "tbl_Institute", new { id=item.INSTITUTE_ID})">
            <i class="fa fa-edit btn btn-success  "></i>
        </a>|
        <a href="@Url.Action("Details", "tbl_Institute", new { id=item.INSTITUTE_ID })">
            <i class="fa fa-book btn btn-warning"></i>
        </a>|
        <a href="@Url.Action("Delete", "tbl_Institute", new { id=item.INSTITUTE_ID })">
            <i class="fa fa-trash btn btn-danger"></i>
        </a>

    </td>
</tr>
    }

</table>
